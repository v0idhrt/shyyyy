      "request": {
        "method": "POST",
        "body": {
          "mode": "formdata",
          "formdata": [{ "key": "file", "type": "file", "src": "/path/to/plan.svg" }]
        },
        "url": {
          "raw": "{{baseUrl}}/api/v1/convert",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "convert"]
        }
      }
    },
    {
      "name": "Converter: JSON -> SVG",
      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" }
        ],
        "body": {
          "mode": "raw",
          "raw": "{ \"sample\": \"put scene json here\" }"
        },
        "url": {
          "raw": "{{baseUrl}}/api/v1/render",
