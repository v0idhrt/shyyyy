{
  "info": {
    "_postman_id": "4c99c57d-4df1-4e77-9f5d-b78f74b588c2",
    "name": "shyyyy API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "description": "Коллекция для gateway (порт 3000)"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{token}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000"
    },
    {
      "key": "userId",
      "value": "11111111-1111-1111-1111-111111111111"
    },
    {
      "key": "token",
      "value": ""
    }
  ],
  "item": [
    {
      "name": "Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"login\": \"admin\",\n  \"password\": \"admin\"\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/v1/login",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "login"]
        }
      }
    },
    {
      "name": "Get Profile",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/api/v1/users/{{userId}}",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}"]
        }
      }
    },
    {
      "name": "List Files",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/api/v1/users/{{userId}}/files",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}", "files"]
        }
      }
    },
    {
      "name": "Get SVG",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/api/v1/users/{{userId}}/svg?name=1.svg",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}", "svg"],
          "query": [{ "key": "name", "value": "1.svg" }]
        }
      }
    },
    {
      "name": "Get PNG",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/api/v1/users/{{userId}}/png?name=1.png",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}", "png"],
          "query": [{ "key": "name", "value": "1.png" }]
        }
      }
    },
    {
      "name": "Get PDF",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/api/v1/users/{{userId}}/pdf",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}", "pdf"]
        }
      }
    },
    {
      "name": "Get JSON",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/api/v1/users/{{userId}}/json?name=data.json",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}", "json"],
          "query": [{ "key": "name", "value": "data.json" }]
        }
      }
    },
    {
      "name": "Upload Edited JSON",
      "request": {
        "method": "POST",
        "body": {
          "mode": "formdata",
          "formdata": [{ "key": "file", "type": "file", "src": "/path/to/data.json" }]
        },
        "url": {
          "raw": "{{baseUrl}}/api/v1/users/{{userId}}/json-edited",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}", "json-edited"]
        }
      }
    },
    {
      "name": "Get Edited JSON",
      "request": {
        "method": "GET",
