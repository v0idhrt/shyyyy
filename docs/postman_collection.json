          "raw": "{{baseUrl}}/api/v1/users/{{userId}}/json-edited?name=data.json",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}", "json-edited"],
          "query": [{ "key": "name", "value": "data.json" }]
        }
      }
    },
    {
      "name": "Upload SVG",
      "request": {
