          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}", "png"]
        }
      }
    },
    {
      "name": "Upload PDF",
      "request": {
        "method": "POST",
        "body": {
          "mode": "formdata",
          "formdata": [{ "key": "file", "type": "file", "src": "/path/to/document.pdf" }]
        },
        "url": {
          "raw": "{{baseUrl}}/api/v1/users/{{userId}}/pdf",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}", "pdf"]
        }
      }
    },
