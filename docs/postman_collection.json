          "formdata": [{ "key": "file", "type": "file", "src": "/path/to/data.json" }]
        },
        "url": {
          "raw": "{{baseUrl}}/api/v1/users/{{userId}}/json",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}", "json"]
        }
      }
    },
    {
      "name": "Upload Edited SVG",
      "request": {
        "method": "POST",
        "body": {
          "mode": "formdata",
          "formdata": [{ "key": "file", "type": "file", "src": "/path/to/edited.svg" }]
        },
        "url": {
          "raw": "{{baseUrl}}/api/v1/users/{{userId}}/svg-edited",
          "host": ["{{baseUrl}}"],
          "path": ["api", "v1", "users", "{{userId}}", "svg-edited"]
        }
      }
